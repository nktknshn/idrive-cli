{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "strict": true,
    "outDir": "dist",
    "rootDir": "./src"
  },
  "exclude": [
    "**/*.spec.ts"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
